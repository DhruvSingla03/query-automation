# Environment Configuration Template
ENV=dev

# Direct database connection (Oracle)
DB_HOST=hostname
DB_PORT=port
DB_DATABASE=database  # Oracle service name or SID
DB_USERNAME=username
DB_PASSWORD=password

# Vault config (not used in dev mode but required)
VAULT_ADDR=https://vault.example.com
VAULT_ROLE_ID=dev
VAULT_SECRET_ID=dev
VAULT_PATH_FASTAG_ACQ=secret/data/fastag/database